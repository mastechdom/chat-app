tp://localhost:2410/${this.chatHistoryHeading}`)\n            .then((resp)=>{\n            console.log(resp.data);\n            this.$store.commit(\"setState\",resp.data);\n      })\n    }\n    }, 2000)\n  },\n  beforeDestroy() {\n    clearInterval(this.timer)\n  }\n})\nexport default class Home extends Vue {\n  name= \"\"\n  enter() {\n    const newName = this.name;\n    if (this.name !== \"\") {\n      console.log(newName)\n      this.$store.commit(\"setUser\", newName);\n      this.name = \"\";\n    }\n  }\n  data() {\n    return {\n      timer: null\n    }\n  }\n}\n</script>\n<style>\n.heading {\n  text-align: center;\n}\n.nameField {\n  text-align: center;\n}\n.button {\n  margin-top: 10px;\n}\n</style>\n"]}]}